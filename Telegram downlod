    }
    h1 {
      color: #333;
    }
    #downloadBtn {
      background-color: #28a745;
      color: white;
      border: none;
      padding: 10px 20px;
      font-size: 16px;
      border-radius: 5px;
      cursor: not-allowed;
    }
    #downloadBtn:enabled {
      cursor: pointer;
      background-color: #007bff;
    }
    #downloadBtn:enabled:hover {
      background-color: #0056b3;
    }
  </style>
  <script>
    function startCountdown() {
      var countdown = 30;
      var downloadButton = document.getElementById("downloadBtn");
      var timer = setInterval(function() {
        if (countdown === 0) {
          clearInterval(timer);
          downloadButton.innerHTML = "Download Now";
          downloadButton.disabled = false;
          downloadButton.onclick = function() {
            window.location.href = "https://raw.githubusercontent.com/cyan-2048/_store/main/arma7x/telekram/telekram.zip";
          };
        } else {
          downloadButton.innerHTML = "Wait " + countdown + " seconds to download";
          downloadButton.disabled = true;
        }
        countdown--;
      }, 1000);
    }

    window.onload = function() {
      startCountdown();
    }
  </script>
</head>
<body>
  <h1>Download Telegram</h1>

  <!-- Download button -->
  <button id="downloadBtn" disabled>Wait 30 seconds to download</button>
